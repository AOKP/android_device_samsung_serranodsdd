From b67241647a1d34a021581a940c1521d45cfd7fb1 Mon Sep 17 00:00:00 2001
From: k2wl <k2wl@github.com>
Date: Tue, 30 Sep 2014 14:09:31 +0530
Subject: [PATCH] add support for S4 mini duos ril class

Change-Id: I6db4da434b086eba34606646b019edcdbff4bb1b
Signed-off-by: k2wl <k2wl@github.com>
---
 BoardConfigCommon.mk | 2 +-
 rootdir/init.qcom.rc | 2 +-
 serrano-common.mk    | 2 +-
 3 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/BoardConfigCommon.mk b/BoardConfigCommon.mk
index 6699b3b..5f0c350 100644
--- a/BoardConfigCommon.mk
+++ b/BoardConfigCommon.mk
@@ -96,4 +96,4 @@ BOARD_USES_QC_TIME_SERVICES := true
 TARGET_POWERHAL_VARIANT :=
 
 # Custom RIL class
-BOARD_RIL_CLASS := ../../../device/samsung/serrano-common/ril/
+BOARD_RIL_CLASS := ../../../device/samsung/serranodsdd/ril/
diff --git a/rootdir/init.qcom.rc b/rootdir/init.qcom.rc
index 4366823..4d530d1 100644
--- a/rootdir/init.qcom.rc
+++ b/rootdir/init.qcom.rc
@@ -455,7 +455,7 @@ service charger /charger
 on property:persist.radio.multisim.config=dsds
     stop ril-daemon
     start ril-daemon
-    start ril-daemon1
+    start ril-daemon2
 
 on property:sys.boot_completed=1
     write /sys/module/rpm_resources/enable_low_power/L2_cache 1
diff --git a/serrano-common.mk b/serrano-common.mk
index 543d537..9c36e96 100644
--- a/serrano-common.mk
+++ b/serrano-common.mk
@@ -176,7 +176,7 @@ PRODUCT_PROPERTY_OVERRIDES += \
 
 # RIL
 PRODUCT_PROPERTY_OVERRIDES += \
-    ro.telephony.ril_class=SamsungMSM8930RIL
+    ro.telephony.ril_class=SerranoDSRIL
 
 # call common msm8930
 $(call inherit-product, device/samsung/msm8930-common/msm8930.mk)
-- 
